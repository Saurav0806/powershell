pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Build and Test') {
            steps {
                script {
                    dockerImage = docker.build("docker-image:latest")
                }
                script {
                    docker.withRun("-p 8040:80 docker-image:latest")
                    }
                }
            
        }

        
    }
}
